{
  "rewrites": [
    {
      "source": "/redis/:path*",
      "destination": "https://try.redis.io/:path*"
    },
    {
      "source": "/completions",
      "destination": "https://api.openai.com/v1/chat/completions"
    },
    {
      "source": "/ai",
      "destination": "https://api.openai.com"
    }
  ],
  "routes": [
    {
      "src": "/ai",
      "status": 308,
      "headers": {
        "Location": "https://api.openai.com"
      }
    }
  ]
}
